{
  "name": "nuclide-onboarding",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Provides the new user experience for new Nuclide users.",
  "author": "Nuclide : Core UI",
  "atomTestRunner": "../../lib/test-runner-entry.js",
  "activationHooks": [
    "nuclide_onboarding:gk:nuclide"
  ],
  "nuclide": {
    "configMetadata": {
      "pathComponents": [
        "IDE",
        "Onboarding"
      ]
    },
    "config": {
      "showOnboarding": {
        "title": "Show Onboarding Pane",
        "type": "boolean",
        "default": true,
        "description": "Show the onboarding pane (by default shown on first startup)."
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-onboarding.onboardingFragments": {
      "versions": {
        "0.0.0": "setOnboardingFragments"
      }
    }
  }
}
