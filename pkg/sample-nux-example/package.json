{
  "name": "sample-nux-example",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "A package demonstrating usage of NUXes within Nuclide.",
  "author": "NEEDS OWNER",
  "atomTestRunner": "../../lib/test-runner-entry.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^1.0.0": "consumeToolBar"
      }
    },
    "nux-register": {
      "versions": {
        "0.0.0": "consumeRegisterNuxService"
      }
    },
    "nux-trigger": {
      "versions": {
        "0.0.0": "consumeTriggerNuxService"
      }
    }
  }
}
